r\n  console.log(\"mtg Annual\", mtgPaymentAnnual);\r\n\r\n  const nt = 12 * 25;\r\n  const rn = 0.0479 / 12;\r\n  const mortgage =\r\n    (mtgPaymentMonthly * (Math.pow(1 + rn, nt) - 1)) /\r\n    (rn * Math.pow(1 + rn, nt));\r\n  console.log(mortgage);\r\n\r\n  const handleName = (e) => {\r\n    setName(e.target.value);\r\n  };\r\n  const handleAge = (e) => {\r\n    setAge(Number(e.target.value));\r\n  };\r\n\r\n  const handleLocation = (e) => {\r\n    SetLocation(e.target.value);\r\n  };\r\n\r\n  const handleIncome = (e) => {\r\n    setIncome(Number(e.target.value));\r\n  };\r\n  const handleLiabilities = (e) => {\r\n    setLiabilities(Number(e.target.value));\r\n  };\r\n  const handlePropertyTax = (e) => {\r\n    setPropertyTax(Number(e.target.value));\r\n  };\r\n  const handleHeatingCost = (e) => {\r\n    setHeatingCost(Number(e.target.value));\r\n  };\r\n  const handleStrata = (e) => {\r\n    setStrata(Number(e.target.value));\r\n  };\r\n  const CalculateTDS = (e) => {\r\n    const mtgPaymentMonthly = (0.44 * income - totalExpenses) / 12;\r\n    const totalAnnualExpenses =\r\n      (liabilities + propertyTax + heatingCost + strata + mtgPaymentMonthly) *\r\n      12;\r\n    setTDS((totalAnnualExpenses / income) * 100);\r\n  };\r\n  const mortgageAmt = (e) => {\r\n    const totalExpenses =\r\n      (liabilities + propertyTax + heatingCost + strata) * 12;\r\n    const mtgPaymentMonthly = (0.44 * income - totalExpenses) / 12;\r\n    const nt = 12 * 25;\r\n    const rn = 0.0479 / 12;\r\n    setResult(\r\n      (\r\n        (mtgPaymentMonthly * (Math.pow(1 + rn, nt) - 1)) /\r\n        (rn * Math.pow(1 + rn, nt))\r\n      ).toFixed(2)\r\n    );\r\n  };\r\n  const mortgagePayment = (e) => {\r\n    const totalExpenses =\r\n      (liabilities + propertyTax + heatingCost + strata) * 12;\r\n    setPayment(((0.44 * income - totalExpenses) / 12).toFixed(2));\r\n  };\r\n\r\n  const totalAnnualExpenses = mtgPaymentAnnual + totalExpenses;\r\n  console.log(\"total annual expenses\", totalAnnualExpenses);\r\n  const ratio = (totalAnnualExpenses / income) * 100;\r\n\r\n  const captureData = {\r\n    name: name,\r\n    age: age,\r\n    location: location,\r\n    income: income,\r\n    liabilities: liabilities,\r\n    mortgage: result,\r\n    mortgageInstallment: payment,\r\n  };\r\n  console.log(\"CaptureData\", captureData);\r\n  const postData = (e) => {\r\n    Axios.post(`${API_URL}/api/start`, captureData)\r\n\r\n      .then((response) => {\r\n        alert(\"Saved! Please click NEXT to proceed\");\r\n        props.id(response.data.id);\r\n      })\r\n\r\n      .catch((error) => {\r\n        console.log(error);\r\n      });\r\n  };\r\n  return (\r\n    <>\r\n    \r\n\r\n      <h2 className=\"mtgCalculator__title\">\r\n        Find Out Mortgage Amount You Can Be Eligible For\r\n      </h2>\r\n      <div className=\"pageWrapper\">\r\n      <main className=\"mtgCalculator\">\r\n        <section className=\"mtgCalculator__container\">\r\n          <form\r\n            className=\"mtgCalculator__form\"\r\n            onSubmit={(e) => e.preventDefault(e)}\r\n          >\r\n            <label className=\"start__form start__form--label\">Name </label>\r\n            <input\r\n              type=\"text\"\r\n              name=\"name\"\r\n              placeholder=\"Name\"\r\n              onChange={handleName}\r\n            />\r\n\r\n            <label className=\"start__form start__form--label\">Age</label>\r\n            <input\r\n              type=\"number\"\r\n              name=\"age\"\r\n              placeholder=\"Age\"\r\n              onChange={handleAge}\r\n            />\r\n            <label className=\"start__form start__form--label\">Location</label>\r\n            <input\r\n              type=\"text\"\r\n              name=\"location\"\r\n              placeholder=\"Location\"\r\n              onChange={handleLocation}\r\n            />\r\n\r\n            <label>Annual Income</label>\r\n            <input\r\n              type=\"number\"\r\n              name=\"income\"\r\n              placeholder=\"Annual Income\"\r\n              onChange={handleIncome}\r\n            ></input>\r\n\r\n            <label>Monthly Liabilities</label>\r\n            <input\r\n              type=\"number\"\r\n              name=\"expenses\"\r\n              placeholder=\"Monthly Liabilities\"\r\n              placeholder=\"Loan,Credit Cards,etc.\"\r\n              onChange={handleLiabilities}\r\n            ></input>\r\n\r\n            <label>Monthly Housing Cost(of the New property) </label>\r\n            <input\r\n              type=\"number\"\r\n              name=\"property taxes\"\r\n              placeholder=\"Property Taxes\"\r\n              onChange={handlePropertyTax}\r\n            ></input>\r\n            <input\r\n              type=\"number\"\r\n              name=\"Heating Cost\"\r\n              placeholder=\"Heating Costs\"\r\n              onChange={handleHeatingCost}\r\n            ></input>\r\n            <input\r\n              type=\"number\"\r\n              name=\"strata\"\r\n              placeholder=\"1/2Monthly Strata Fees\"\r\n              onChange={handleStrata}\r\n            ></input>\r\n            <div className=\"mtgCalculator__buttonList\">\r\n              <button\r\n                className=\"mtgCalculator__buttonListItem\"\r\n                onClick={() => {\r\n                  mortgagePayment();\r\n                  mortgageAmt();\r\n                  CalculateTDS();\r\n                }}\r\n              >\r\n                Calculate\r\n              </button>\r\n              <button\r\n                onClick={postData}\r\n                className=\"mtgCalculator__buttonListItem\"\r\n              >\r\n                Save\r\n              </button>\r\n            </div>\r\n\r\n            <div className=\"mtgCalculator__result\">\r\n              {payment >= 0 ? (\r\n                <div className=\"mtgCalculator__result mtgCalculator__result__items\">\r\n                  <p>You can afford monthly payments of: <i className=\"result\"> ${payment}</i></p>\r\n                  <p> Mortgage amount that you can afford: <i className=\"result\">${result} </i></p>\r\n                </div>\r\n              ) : (\r\n                <div>Income to Debt ratio is too high!</div>\r\n              )}\r\n            </div>\r\n          </form>\r\n        </section>\r\n\r\n        <section className=\"mtgCalculator__tips\">\r\n          <h3>Tips/Hints:</h3>\r\n          <ul className=\"mtgCalculator__tipsList\">\r\n            <li className=\"mtgCalculator__tipsListItems\">\r\n              <h5>Monthly Liabilities:</h5> A liability is an obligation between\r\n              one party and another not yet completed or paid for. Monthly\r\n              Liabilities refer to amount one pays on monthly basis towards\r\n              these obligations. For the purpose of this calculator enter sum\r\n              total of all the monthly payments you make towards:\r\n              <ul className=\"mtgCalculator__tipsListSub\">\r\n                <li>Credit Cards</li>\r\n                <li>Loans such as auto loans, etc.</li>\r\n                <li>Lines of Credit</li>\r\n                <li>Alimony,Child Support</li>\r\n              </ul>\r\n              <li className=\"mtgCalculator__tipsListItems\">\r\n                <h5>Unutilized Credit:</h5>For any credit approved but not\r\n                utilised yet add 5% of the credit limit as monthly payment.\r\n                Example: If the credit limit of a credit card is $5000 but the\r\n                balance owing is $0; the monthly payment taken into account\r\n                should be $250.\r\n              </li>\r\n            </li>\r\n            <li className=\"mtgCalculator__tipsListItems\">\r\n              <h5>Annual Income:</h5> Enter average of most recent 2 years\r\n              income from Line 150 of{\" \"}\r\n              <p className=\"info\" href=\"#\" onClick={() => setIsOpen(true)}>\r\n                {\" \"}\r\n                Notice of Assessment\r\n                <sup className=\"sup\">\r\n                  <FontAwesomeIcon icon={faInfoCircle} />\r\n                </sup>\r\n              </p>\r\n            </li>\r\n            <li className=\"mtgCalculator__tipsListItems\">\r\n              <h5>Monthly Housing Cost:</h5> Mortgage Affordability takes into\r\n              account the expenses incurred with regards to the property you\r\n              intend to purchase. They include: Property Taxes, Heating Cost and\r\n              half monthly Strata Fees. - Property Taxes: Are paid annually\r\n              therefore, annual amount is mentioned on the listings.Enter the\r\n              amount divided by twelve.\r\n            </li>\r\n            <li>\r\n              <h5>Strata Fee:</h5>\r\n              The strata fee is comprised of two separate funds – an Operating\r\n              Fund and a Contingency Reserve Fund (CRF). These are mandatory for\r\n              all strata corporations in British Columbia.\r\n            </li>\r\n          </ul>\r\n\r\n          <Modal open={isOpen} onClose={() => setIsOpen(false)}></Modal>\r\n        </section>\r\n      </main>\r\n      <div className=\"nextPreviousIcons\">\r\n        <Link to=\"/homepage\">\r\n          <img className=\"nextPreviousIcons__items\" src={ArrowBack} />\r\n        </Link>\r\n\r\n        <Link to=\"/savingscalculator\">\r\n          <img className=\"nextPreviousIcons__items\" src={ArrowForward} />\r\n        </Link>\r\n      </div>\r\n      </div>\r\n    </>\r\n  );\r\n}\r\n","C:\\Users\\HARGUL\\Desktop\\Brainstation\\Capstone_2\\client\\src\\Pages\\Homepage.js",[],"C:\\Users\\HARGUL\\Desktop\\Brainstation\\Capstone_2\\client\\src\\Pages\\SavingsCalculator.js",["199","200","201"],"import Axios from 'axios';\r\nimport React, { useState } from 'react';\r\nimport {Link} from 'react-router-dom';\r\nimport ArrowBack from '../assets/icons/ArrowBack.svg';\r\nimport ArrowForward from '../assets/icons/ArrowForward.svg';\r\nconst API_URL = process.env.NODE_ENV === \"production\"\r\n  ? 'https://homebuddy-app.herokuapp.com'\r\n  : 'http://localhost:5000';\r\n\r\n\r\nexport default function SavingsCalculator(props) {\r\n    const [savings, setSavings] = useState(null);\r\n    const [contributions, setContributions] = useState(null);\r\n    const [interest, setInterest] = useState(null);\r\n    const [compounds, setCompounds] = useState(null);\r\n    const [years, setYears] = useState(null);\r\n    const [futureValue, setFutureValue] = useState(null);\r\n    const [getData, setGetData] = useState(null);\r\n  \r\n\r\n    const handleSavings = (e) => {\r\n        setSavings(Number(e.target.value))\r\n    }\r\n    const handleContributions = (e) => {\r\n        setContributions(Number(e.target.value))\r\n    }\r\n    const handleInterest = (e) => {\r\n        setInterest(Number(e.target.value))\r\n    }\r\n    const handleCompounds = (e) => {\r\n        setCompounds(Number(e.target.value))\r\n    }\r\n    const handleYears = (e) => {\r\n        setYears(Number(e.target.value))\r\n    }\r\n    const rn = (interest/100)/compounds\r\n    console.log(\"rn\",rn)\r\n  \r\n\r\n    \r\n    const nt = compounds*years\r\n    console.log(\"nt\",nt)\r\n    \r\n    const interestOnPrincipal = savings*(Math.pow((1+rn),nt))\r\n    \r\n    \r\n    const numerator = ((Math.pow((1+rn),(nt)))-1)/rn\r\n    const numerator2 = numerator*(contributions*12)\r\n    console.log(\"numerator2\",numerator2)\r\n console.log(\"contributions\",contributions)\r\n\r\n    const rhs= (interestOnPrincipal+numerator2).toFixed(2)\r\n   \r\n    const FutureValue = (e) => {\r\n        setFutureValue(rhs)\r\n    }\r\n \r\n\r\n\r\n   \r\n    const patchData =(e)=>{\r\n            const editData = {\r\n            savings:savings,\r\n            contributions:contributions,\r\n            interest:interest,\r\n            years:years,\r\n            futureValue:futureValue,\r\n        }\r\n        console.log('edit data',editData)\r\n        Axios.put(`${API_URL}/api/start/${props.id}`,editData)\r\n        .then((response)=>{alert(\"Saved! Please click NEXT to proceed\")\r\n            setGetData(response.editData)})\r\n        .catch((error)=>{console.log(error)})\r\n\r\n    }\r\n\r\n\r\n    return (\r\n        <>\r\n        <h2 className=\"savings__title\">\r\n                Start Saving For DownPayment And Other Expenses\r\n        </h2>\r\n        <div className=\"pageWrapper\">\r\n        <main className=\"savings\">\r\n          \r\n        <section className=\"savings__calculator\">\r\n            <form className=\"savings__form\" onSubmit={(e) => e.preventDefault(e)}>\r\n        \r\n        <label className=\"savings__formLabel\"> Current Savings </label>\r\n        <input type=\"number\" name=\"savings balance\"onChange={handleSavings} ></input>\r\n       \r\n        \r\n        <label> Monthly Contributions</label>\r\n        <input type=\"number\" name=\"monthly contributions\"onChange={handleContributions}></input>\r\n      \r\n        \r\n        <label> Annual Interest Rate</label>\r\n        <input type=\"number\" name=\"annual interest rate\"onChange={handleInterest} ></input>\r\n        \r\n        \r\n        <label>Compounds Per Year </label>\r\n        <input type=\"number\" name=\"number of compounds per year\" onChange={handleCompounds}></input>\r\n       \r\n        \r\n        <label> Number of years</label>\r\n        <input type=\"number\" name=\"number of years\"onChange={handleYears}></input>\r\n        \r\n        <div className=\"savings__formButtons\">\r\n        <button className=\"savings__button\" onClick={FutureValue}>Calculate</button>\r\n        \r\n        <button className=\"savings__button\"onClick={patchData}>Save</button>\r\n        </div>\r\n        <p className=\"savings__result\"> Future Value of Your Investment:<i className=\"result\">${futureValue}</i>\r\n        </p>\r\n        \r\n        </form>\r\n        </section>\r\n        <section className=\"savings__tips\">\r\n            <h3>Tips/Hints:</h3>\r\n            <ul className=\"savings__tipsList\">\r\n                <li className=\"savings__tipsListItems\">\r\n                    <h5>Future Value:</h5>\r\n                    Future value (FV) is the value of a current asset at a future date based on an assumed rate of growth(interest on investment). Knowing the future value enables individuals to make sound investment decisions based on their anticipated needs.\r\n                </li>\r\n                <li><h5>Compounds Per Year: </h5>Refers to number of times interest on your investment is added to the invested amount in a year.Example: Investment of $100 with interest rate of 5%  compounded quarterly translates into receiving interest of 5% four times in a year.\r\n                </li>\r\n                <li>\r\n                    <h5>Savings/Investment Options:</h5>\r\n                    Savings Accounts are a safe bet to accumulate your funds for downpayment. \r\n                    However, if you plan to invest in real estate a few years from now it is worth looking into other options that pay higher interest than Savings accounts such as Guaranteed Investment Certificate(GIC)\r\n                </li>\r\n                <li><h5>Miscellaneous Expenses</h5>\r\n                Account for miscellaneous expenses when purchasing real estate.Budget for 1-2% of the home purchase price for the following: \r\n                <ul className=\"savings__tipsListSub\">\r\n                    <li>Appraisal</li>\r\n                    <li>Home Inspection</li>\r\n                    <li>Title Insurance</li>\r\n                    <li>Legal</li>\r\n                    <li>Taxes</li>\r\n                </ul> \r\n                </li>\r\n            </ul>\r\n      \r\n          \r\n        </section>\r\n       \r\n    </main>\r\n    <div className=\"nextPreviousIcons\">\r\n            \r\n         